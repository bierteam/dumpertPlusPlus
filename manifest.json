{

  "manifest_version": 2,
  "name": "Dumpert++",
  "version": "0.2.3",
  "author": "Bier Team",
  "homepage_url": "https://github.com/bierteam/dumpertPlusPlus",

  "description": "Fix dumpert view status and scroll location bug",

  "permissions": ["storage"],
  "content_scripts": [
    {

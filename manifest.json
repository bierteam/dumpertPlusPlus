{

  "manifest_version": 2,
  "name": "Dumpert++",
  "version": "0.1.1",

  "description": "Fix dumpert view status",

  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.dumpert.nl/*"],
      "js": [
        "fix.js"
      ],
      "run_at": "document_start"
    }
  ]
}